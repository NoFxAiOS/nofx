# 环境变量配置文件（服务器部署用）
# 将此文件重命名为 .env 并填入真实的密钥

# DeepSeek API 密钥
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Aster 交易所私钥
ASTER_PRIVATE_KEY=your_aster_private_key_here

# 可选：自定义端口
BACKEND_PORT=8080
FRONTEND_PORT=3000

# 可选：时区设置
TZ=Asia/Shanghai

# 可选：日志级别
LOG_LEVEL=info

# 可选：数据库连接（如果将来需要）
# DATABASE_URL=

# 可选：Redis 缓存（如果将来需要）
# REDIS_URL=